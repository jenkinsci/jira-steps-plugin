<?xml version="1.0" encoding="UTF-8"?>
<?jelly escape-by-default='true'?>
<j:jelly
  xmlns:f="/lib/form"
  xmlns:j="jelly:core"
>
  <f:entry field="idOrKey" title="Issue idOrKey">
    <f:textbox/>
  </f:entry>
  <f:entry field="linkId" title="Remote Link Id">
    <f:textbox/>
  </f:entry>
  <f:advanced>
    <f:entry field="site" title="Site Name">
      <f:select/>
    </f:entry>
    <f:entry field="failOnError">
      <f:checkbox default="true" title="Fail On Error"/>
    </f:entry>
  </f:advanced>
  <f:block>
    <p>See <a
      href="https://jenkinsci.github.io/jira-steps-plugin/steps/issuelink/jira_get_remote_issuelink/"
      target="_blank">jiraGetIssueRemoteLink
    </a> for more information.
    </p>
  </f:block>
</j:jelly>